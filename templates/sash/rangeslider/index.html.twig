{% endblock %}

{% block javascripts %}
   {{parent()}} 

    <!-- INTERNAL ion.rangeSlider.min js -->
    <script src="{{absolute_url(asset('build/plugins/ion-rangeslider/js/ion.rangeSlider.min.js'))}}"></script>
